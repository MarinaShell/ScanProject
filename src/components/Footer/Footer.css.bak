@font-face {
    font-family: 'Inter';
    src: url('Inter-Regular.otf')
}

footer {
    position: absolute;
    left: 0px;
    bottom: 0px;
    height: 137px;
    width: 100%;
    background: #029491;

.foot {
    display: flex;
    flex-direction: column;
}

.Logo {
    position: absolute;
    width: 141px;
    height: 141px;
    left: 60px;
    top: -2px;
}

.foot_text {
    font-family: 'Inter';
    text-align: right;
    margin-right: 45px;
    color: white;
    margin-top: 25px;
    font-size: 14px;
}

@media screen and (max-width: 600px) {
    footer {
        height: 100px;
    }

    .Logo {
        width: 111px;
        height: 111px;
        left: 18px;
        top: -12px;
    }

    .foot_text {
        margin-right: 15px;
        margin-top: 15px;
        font-size: 11px;
    }
}

